{
  "name": "@apparts/model-api",
  "version": "6.4.5",
  "description": "Generate CRUDish API-endpoints for a model defined with the @apparts/model package",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**/*",
    "src/**/*"
  ],
  "scripts": {
    "test": "vitest --globals --exclude './build/**'",
    "prepublishOnly": "npm run ci && npm run clean && npm run build",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "clean": "rimraf coverage build tmp",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true npm run test",
    "ci:eslint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phuhl/apparts-model-api"
  },
  "keywords": [
    "apparts",
    "model",
    "api"
  ],
  "author": "Philipp Uhl",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/phuhl/apparts-model-api/issues"
  },
  "peerDependencies": {
    "@apparts/model": "^7.5.0",
    "@apparts/prep": "^2.1.3 || ^3",
    "@apparts/types": "^9.14.1 || ^10.0.0"
  },
  "devDependencies": {
    "@apparts/backend-test": "^4.2.1",
    "@apparts/config": "^1.1.0",
    "@apparts/db": "^3.11.0",
    "@apparts/model": "^7.5.0",
    "@apparts/prep": "^3.1.0",
    "@apparts/types": "^10.0.0",
    "@eslint/js": "^9.34.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^22.18.0",
    "@types/supertest": "^6.0.3",
    "@vitest/eslint-plugin": "^1.3.7",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "jsonwebtoken": "^9.0.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsutils": "^3.21.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "homepage": "https://github.com/phuhl/apparts-model-api#readme",
  "volta": {
    "node": "22.14.0",
    "npm": "10.9.2"
  }
}
